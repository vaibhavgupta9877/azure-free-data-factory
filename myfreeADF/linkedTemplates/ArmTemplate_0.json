{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "myfreeADF"
		},
		"AzureDataLakeStorageFree_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageFree'"
		},
		"AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://mykeyvaultfree.vault.azure.net/"
		},
		"AzureDataLakeStorageFree_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://myazurefreetier.dfs.core.windows.net/"
		},
		"AzureDatabricksFreeTier_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0126-082329-hrkxkz7k"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AzureKeyVault')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureKeyVault",
				"typeProperties": {
					"baseUrl": "[parameters('AzureKeyVault_properties_typeProperties_baseUrl')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureDataLakeStorageFree')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('AzureDataLakeStorageFree_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('AzureDataLakeStorageFree_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureDatabricksFreeTier')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureDatabricks",
				"typeProperties": {
					"domain": "https://adb-4052026038755900.0.azuredatabricks.net",
					"authentication": "MSI",
					"workspaceResourceId": "/subscriptions/f4ccc37d-1601-4c1a-8441-eee88f3252aa/resourceGroups/AzureFreeTierRG/providers/Microsoft.Databricks/workspaces/ADBFreeTier",
					"existingClusterId": "[parameters('AzureDatabricksFreeTier_properties_typeProperties_existingClusterId')]"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureKeyVault')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/CNEOS_Data_Processed')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureDataLakeStorageFree",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "Parquet",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": "processedData.parquet",
						"folderPath": "processedData",
						"fileSystem": "cneosproject"
					},
					"compressionCodec": "snappy"
				},
				"schema": [
					{
						"name": "Designation",
						"type": "UTF8"
					},
					{
						"name": "Close_Approach_Date_formatted",
						"type": "UTF8"
					},
					{
						"name": "Orbit_Id",
						"type": "UTF8"
					},
					{
						"name": "Nominal_Approch_distance_au",
						"type": "DOUBLE"
					},
					{
						"name": "Nominal_Approch_distance_km",
						"type": "DOUBLE"
					},
					{
						"name": "Min_Close_Approach_Distance_au",
						"type": "DOUBLE"
					},
					{
						"name": "Min_Close_Approach_Distance_km",
						"type": "DOUBLE"
					},
					{
						"name": "Max_Close_Approach_Distance_au",
						"type": "DOUBLE"
					},
					{
						"name": "Max_Close_Approach_Distance_km",
						"type": "DOUBLE"
					},
					{
						"name": "V_Reletive_Kms",
						"type": "DOUBLE"
					},
					{
						"name": "V_Infinite_Kms",
						"type": "DOUBLE"
					},
					{
						"name": "Absolute_Magnitude_mag",
						"type": "DOUBLE"
					},
					{
						"name": "Diameter_Km",
						"type": "UTF8"
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureDataLakeStorageFree')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/CNEOS_Data_ELTL_Pipeline')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "CNEOS_Data_Extractor",
						"type": "DatabricksNotebook",
						"dependsOn": [],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"notebookPath": "/Users/vaibhavgupta9877@gmail.com/CNEOS_Data/ADF-CNEOS-Data"
						},
						"linkedServiceName": {
							"referenceName": "AzureDatabricksFreeTier",
							"type": "LinkedServiceReference"
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {},
					"cancelAfter": {}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureDatabricksFreeTier')]"
			]
		}
	]
}