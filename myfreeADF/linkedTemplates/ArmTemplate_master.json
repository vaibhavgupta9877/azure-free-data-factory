{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "myfreeADF"
		},
		"AzureDataLakeStorageFree_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageFree'"
		},
		"CneosSQLDatabaseFreeTier_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CneosSQLDatabaseFreeTier'"
		},
		"AzureDataLakeStorageFree_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://myazurefreetier.dfs.core.windows.net/"
		},
		"AzureDatabricksFreeTier_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0126-082329-hrkxkz7k"
		},
		"AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://mykeyvaultfree.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "myfreeADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageFree_accountKey": {
						"value": "[parameters('AzureDataLakeStorageFree_accountKey')]"
					},
					"CneosSQLDatabaseFreeTier_connectionString": {
						"value": "[parameters('CneosSQLDatabaseFreeTier_connectionString')]"
					},
					"AzureDataLakeStorageFree_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageFree_properties_typeProperties_url')]"
					},
					"AzureDatabricksFreeTier_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricksFreeTier_properties_typeProperties_existingClusterId')]"
					},
					"AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}